---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Club">
  <div class="absolute left-0 overflow-hidden w-full">
    <div
      class="h-[40rem] w-[100dvw] overflow-hidden flex items-center justify-center relative"
    >
      <video
        autoplay
        preload="auto"
        loop
        src="src/assets/videos/skate.mp4"
        class="object-contain overflow-clip w-screen aspect-video"></video>

      <div class="absolute z-10 w-2/3">
        <svg
          class="fill-white w-20"
          id="Capa_1"
